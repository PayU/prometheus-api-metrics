{
  "name": "prometheus-express-metrics",
  "version": "0.1.0",
  "description": "Express routes and process metric collection for Prometheus",
  "author": "Idan Tovi",
  "scripts": {
    "test": "mocha --opts test/mocha.opts",
    "lint": "tslint -c tslint.json --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint --fix -c tslint.json --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "coverage": "nyc npm run test",
    "depcheck": "depcheck",
    "build": "rm -rf ./dist && tsc",
    "pretest": "npm run lint"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "pre-commit": "^1.2.2",
    "prom-client": "11.x",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.1.2",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "depcheck": "^0.9.1",
    "express": "^4.16.4",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "node-fetch": "^2.6.0",
    "node-mocks-http": "^1.8.1",
    "nyc": "^15.0.0",
    "prom-client": "^11.2.1",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.8",
    "rewire": "^4.0.1",
    "sinon": "^8.0.2",
    "sleep": "^6.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-standard-plus": "^2.3.0",
    "typescript": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zooz/prometheus-api-metrics.git"
  },
  "keywords": [
    "monitoring",
    "nodejs",
    "node",
    "prometheus",
    "express",
    "metrics"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Zooz/prometheus-api-metrics/issues"
  },
  "homepage": "https://github.com/Zooz/prometheus-api-metrics#readme",
  "pre-commit": [
    "test"
  ]
}
