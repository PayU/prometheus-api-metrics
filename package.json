{
  "name": "prometheus-api-metrics",
  "version": "2.1.8",
  "description": "API and process monitoring with Prometheus for Node.js micro-service",
  "author": "Idan Tovi",
  "scripts": {
    "unit-tests": "nyc mocha ./test/unit-test/*-test.js",
    "no-koa-test": "nyc mocha ./test/unit-test/*-test.js ./test/integration-tests/express/*-test.js --require ts-node/register test/integration-tests/nest-js/*.spec.ts",
    "test": "nyc mocha ./test/unit-test/*-test.js ./test/**/**/*-test.js --require ts-node/register test/integration-tests/nest-js/*.spec.ts",
    "integration-tests": "nyc mocha ./test/**/**/*-test.js --require ts-node/register test/integration-tests/nest-js/*.spec.ts",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "main": "src/index.js",
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "src/index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "debug": "^3.1.0",
    "pkginfo": "^0.4.1"
  },
  "peerDependencies": {
    "prom-client": "11.x"
  },
  "devDependencies": {
    "@nestjs/common": "^4.6.4",
    "@nestjs/core": "^4.6.4",
    "@nestjs/testing": "^4.6.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/supertest": "^2.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "doctoc": "^1.3.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.3",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "nyc": "^13.1.0",
    "prom-client": "^11.x",
    "reflect-metadata": "0.1.10",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rewire": "^4.0.1",
    "rxjs": "^5.4.2",
    "sinon": "^5.0.10",
    "sleep": "^5.2.4",
    "stryker": "^0.30.1",
    "stryker-api": "^0.21.4",
    "stryker-javascript-mutator": "^0.11.2",
    "stryker-mocha-framework": "^0.12.5",
    "stryker-mocha-runner": "^0.14.5",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zooz/prometheus-api-metrics.git"
  },
  "keywords": [
    "monitoring",
    "nodejs",
    "node",
    "prometheus",
    "api",
    "express",
    "koa",
    "metrics"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Zooz/prometheus-api-metrics/issues"
  },
  "homepage": "https://github.com/Zooz/prometheus-api-metrics#readme"
}
